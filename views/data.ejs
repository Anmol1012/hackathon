		<div class="container">
			<script><let details = 'https://www.makaan.com/<%= json.items[i].listing.resaleURL %>';></script>
			<div class="card" onclick="location.href = details;">
				<div class="cardimg">
					<img src="<%= json.items[i].listing.mainImageURL %>" class="cardimg1">
				
					<div class="sellerrow">
			<img src="<%= json.items[i].listing.companySeller.user.profilePictureURL %>"
						class="slrimg">
						<div class="sellerinfo">
							<div class="sellername"><%= json.items[i].listing.companySeller.user.fullName %></div>
							<div class="rating">
								<%= json.items[i].listing.sellerCallRatingCount + " ratings" %>
							</div>
							<div class="sellertype">
								<% var str = json.items[
									i].listing.listingSellerTransactionStatuses[0];
									var type = str.split('_').join(' ');%>
								<%=	type  %>	
							</div>
							<!-- <div class="rating">
								
							</div> -->
						</div>
					</div>
				</div>
			<div class="buildingdesc">
				<h4 class="name"  ><strong><%= json.items[i].listing.property.bedrooms + " BHK " + json.items[i].listing.property.unitType %>
				</strong>
				<font color="grey">in <%= json.items[i].listing.property.project.builder.name + " " + json.items[i].listing.property.project.name %></font>
				</h4><br>
				<font color="grey">
					<p class="city"><%= json.items[i].listing.property.project.locality.label + "," + json.items[i].listing.property.project.locality.suburb.city.label %>
					</p></font>
				<div class="listing">
					<div class="hcol">
						<div class="price">
							<span>â‚¹ 
							  <% var price = json.items[i].listing.currentListingPrice.price;
								 if((price/10000000)>1) {
								 price = Number((price/10000000).toFixed(2));
								 var temp="Cr."; } 
								 else { 
								 price = Number((price/100000).toFixed(2));
								 var temp="L"; } %>
							
								<%= price %> <%= temp %>
							</span>
						</div>
						<div class="lbl"><%= json.items[i].listing.property.carpetArea +"/" + json.items[i].listing.property.measure %>
							
						</div>
							
					</div>
					<div class="hcol">
						<div class="size">
							<span><%= json.items[i].listing.property.size %>
							</span>
						</div>
						<div class="lbl">Area in
							<%= json.items[i].listing.property.measure %>
							
						</div>	
					</div>
					<div class="hcol w1">
						<div class="val">
							<%= json.items[i].listing.property.
							project.projectStatus %>
						</div>
						<div class="lbl">
							Construction Status
							<img src="/images/link.png" class="link">
						</div>
					</div>	
				</div>
			
			<div class="desc">
				<div class="bathrooms"><%= json.items[i].listing.property.bathrooms + " Bathrooms" + " |" %>
					<%= json.items[i].listing.floor %> <sup>th</sup>
					<%= "of " + json.items[i].listing.totalFloors + "floors" %>
				</div>
				<div class="info">
					<%= json.items[i].listing.description %>
				</div>
				<div class="shortlist">
					<img src="/images/bookmark.png" id="shortlist" class="shortlist1" onclick="changeImage(this)">
					<img src="/images/share.png" class="share">
					<button class="viewphone">View Phone
						
					</button>
				</div>
			</div>
		</div>
		</div>	
	</div>
<!-- </div> -->
